@model List<LoginWithAuthenticationTest.Models.Language>


@{
    ViewBag.Title = "SelectLanguages";
}
<div class="searchProgrammer">
    <h1>Select your expertise</h1>
        <div class="language">

            @foreach (var item in Model)
            {
                <label class="types">
                    <input type="radio">
                    <img src="@item.Image" alt="@item.Name">
                </label>
            }

        </div>
        
     <div class="send">
         @*<input class="submit" type="submit" value="Pesquisar" />*@
         @Html.ActionLink("Submit", "Index", "Home")
     </div>

</div>